@model GalleryModel

<div class="container">
	<div class="row">
		<div class="col">
			<div class="card text-left">
				<div class="card-body">
					<h5 class="card-title">Some awesome pictures of our teams!</h5>
					<p class="card-text">
						Add some info here.
					</p>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<div id="galleria" class="galleria">
				@foreach (var image in Model.ImagePath)
				{
					<a href="gallery/@image">
						<img src="gallery/@image">
					</a>
				}
			</div>
		</div>
	</div>
</div>